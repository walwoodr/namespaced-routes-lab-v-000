<%= form_for [:admin, @preference] do |f| %>
  <%= f.label "Song Sort Order" %><br>
  <%= f.text_field :song_sort_order %><br></br>
  <%= f.label "Artist Sort Order" %><br>
  <%= f.text_field :artist_sort_order %><br></br>
  <%= f.check_box :allow_create_artists %>
  <%= f.label :allow_create_artists, "Can create Artists" %><br></br>
  <%= f.check_box :allow_create_songs %>
  <%= f.label :allow_create_songs, "Can create Songs" %><br></br>
  <%= f.submit %>
<% end %>
